<script data-cfasync="false" src="/cdn-cgi/scripts/5c5dd728/cloudflare-static/email-decode.min.js"></script><script>
    CurrentUser.clearDownloadedBooks()
    CurrentUser.markDownloadedBooks()

    $(document).ready(function() {

        $('.btnDonate').on('click', (e) => {
            let codeInput = $(this).find('input[name="code"]');
            let code = codeInput.val();

            if (code.length === 0) return;

            new ZLibraryResponse('/papi/promocodes/use', {code: code})
                .success(function (data) {
                    if (data.result == "OK") {
                        ZLibraryNotify().success("Promo code applied to your account!");
                        codeInput.val('');
                    } else {
                        codeInput.closest('.code-input').addClass('error');
                    }
                })
                .error((data) => {
                    codeInput.closest('.code-input').addClass('error');
                })
                .fetch();
        });

        $('input[name="code"]').on('focus, click', function() {
            $(this).closest('.code-input').removeClass('error');
        });

        $('input[name="code"]').on('keyup', function() {
            if ($(this).val().length > 0) {
                $(this).closest('.code-input').addClass('active');
            } else {
                $(this).closest('.code-input').removeClass('active');
            }
        });

    });
</script>                                    </div>
