<script>
    (() => {
        let clearAtModalClose = false
        let isLoading = false
        let isLastPage = false
        let page = 1
        let newNotificationsCount = CurrentUser?.notifications_count ?? 0;
        const loadNotificationsTrigger = document.querySelector('.navigation-element.navigation-notifications')
        const loadMoreNotificationsTrigger = loadNotificationsTrigger.querySelector('.simplebar-content-wrapper');
        loadNotificationsTrigger.addEventListener('mouseenter', loadNotifications)
        loadNotificationsTrigger.addEventListener('click', loadNotifications)
        loadMoreNotificationsTrigger.addEventListener('scroll', checkScrollPosition)
        if (newNotificationsCount > 0) document.querySelector('.navigation-notifications .badge').innerHTML = newNotificationsCount;
        function loadNotifications() {
            loadNotificationsTrigger.removeEventListener('mouseenter', loadNotifications)
            loadNotificationsTrigger.removeEventListener('click', loadNotifications)
            if(isLoading) return
            isLoading = true
            new ZLibraryResponse(`/papi/notification/all?page=${page}`)
                .success(data => {
                    isLoading = false
                    for (let note of data.response.data) {
                        appendNotification(note)
                    }
                    if(page == 1 && data.response.data.length == 0){
                        appendEmptyNotice()
                    }
                    if(data.response.meta){
                        isLastPage = data.response.meta.last_page == page
                    }
                    page++;
                    if(document.querySelector('.notifications-list__loader')){
                        document.querySelector('.notifications-list__loader').remove()
                        ZLibrary.DOM.scroll = true
                    }
                })
                .fetch();
        }
        function appendEmptyNotice(){ 
            const html = `<div class="notifications-list__empty">
                <div class="notifications-list__icons-gallery">
                    <div class="notifications-list__icon"><span></span></div>
                    <div class="notifications-list__icon"><span class="zlibicon-flag"></span></div>
                    <div class="notifications-list__icon"><span class="zlibicon-blob"></span></div>
                    <div class="notifications-list__icon"><span class="zlibicon-request"></span></div>
                    <div class="notifications-list__icon"><span class="zlibicon-zalerts"></span></div>
                    <div class="notifications-list__icon"><span class="zlibicon-heart"></span></div>
                    <div class="notifications-list__icon"><span class="zlibicon-book"></span></div>
                    <div class="notifications-list__icon"><span class="zlibicon-top"></span></div>
                    <div class="notifications-list__icon"><span></span></div>
                </div>
                <div class="notifications-list__text"><b>To get notification</b> create your first <a href="/users/zalerts">Z-Alert</a>, subscribe on <a href="/requests">Book Requests</a>, comment on <a href="/booklists">Booklist</a>, <a href="/popular">Books</a> & much more</div>
            </div>`;
            document.querySelector('.notifications-list').innerHTML = '';
            document.querySelector('.notifications-list').insertAdjacentHTML("beforeend", html)
        }
        function checkScrollPosition() {
            if (loadMoreNotificationsTrigger.scrollTop + loadMoreNotificationsTrigger.clientHeight >= loadMoreNotificationsTrigger.scrollHeight - 1) {
                if(!isLastPage && !isLoading){
                    appendLoader()
                    ZLibrary.DOM.scroll = false
                    loadNotifications()
                }
            }
        }
        function appendLoader(){
            const html = `<div class="notifications-list__loader"><img class="loader" src="/img/loader.svg"></div>`
            document.querySelector('.simplebar-content').insertAdjacentHTML("beforeend", html)
            loadMoreNotificationsTrigger.scrollTo({ top: loadMoreNotificationsTrigger.scrollHeight, behavior: 'smooth' })
        }
        function appendNotification(notification) {
            const icons = {
                comment: "zlibicon-blob",
                requests: "zlibicon-request",
                zalert: "zlibicon-zalerts",
                like: "zlibicon-heart",
                winner: "zlibicon-top", 
                thanks: "zlibicon-gratitude-n", 
                'donation-end': "zlibicon-dollar", 
                'request-approved': "zlibicon-request"
            }

            const date = new Date(notification.created_at)
            if (!notification.data.type) {
                return
            }

            const link = notification.data.link ? `href="${notification.data.link}"` : '';
                        
            const html = `<div class="notifications-list__element ${!notification.read ? 'new' : ''}">
                <a ${link}>
                    <div class="notifications-list__icon"><span class="${icons[notification.data.type]}"></span></div>
                    <div class="notifications-list__content">
                        <div class="notifications-list__message">
                            ${i18next.t(notification.message, {user: `<object><a href="${notification.data?.user?.profile}">${notification.data?.user?.name}</a></object>`, title: '<strong>' + notification.data?.title + '</strong>', activeLink: `<object><a href="${notification.data?.activeLink?.link}">${notification.data?.activeLink?.title}</a></object>`, interpolation: { escapeValue: false } })}
                        </div>
                        ${notification.data.quote ? `
                            <div class="notifications-list__quote">${notification.data.quote}</div>
                        `:``}
                        <div class="notifications-list__time">${date.toLocaleString({}, {day: 'numeric', month: 'short',year: 'numeric'})}</div>
                    </div>
                </a>
                ${link ? `<a ${link} target="_blank"><span class="zlibicon-blank-n"></span></a>` : ''}
            </div>`

            document.querySelector('.simplebar-content').insertAdjacentHTML("beforeend", html)
        }

        document.querySelector('.navigation-notifications .navigation-icon').addEventListener('click', function() {
            document.querySelector('.notifications-board').style.display = 'block';

            ZLibrary.DOM.overlay = 'dropdown';
            if (newNotificationsCount > 0) {
                new ZLibraryResponse(new Request('/papi/notification/mark-read-all', {
                    method: 'PATCH'
                }))
                    .success(() => {
                        newNotificationsCount = 0;
                        clearAtModalClose = true
                    })
                    .fetch()
            }
        });

        window.addEventListener('modalClose', function(e) {
            document.querySelector('.notifications-board').classList.add("out");
            ZLibrary.DOM.scroll = true
            setTimeout(() => {
                if(clearAtModalClose){
                    document.querySelectorAll('.notifications-board .new').forEach(el => el.classList.remove('new'));
                    document.querySelector('.navigation-notifications .badge').innerHTML = '';
                    clearAtModalClose = false;
                }
                document.querySelector('.notifications-board').style.display = 'none';
                document.querySelector('.notifications-board').classList.remove("out");
            }, 300);
        });

    })()

</script>        <section class="navigation-element navigation-user-card-element" style="position: relative;">
